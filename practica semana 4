num = int(input("Digite un numero"))
num2 = int(input("Digite un numero"))
num3 = int(input("Digite un numero"))
num4 = int(input("Digite un numero"))

if num > num2 and num > num3 and num > num4:
    print(num, "Es el mayor de esos 4 numeros")
if num2 > num and num2 > num3 and num2 > num4:
    print(num2, "Es el mayor de esos 4 numeros")
if num3 > num2 and num3 > num and num3 > num4:
    print(num3, "Es el mayor de esos 4 numeros")
if num4 > num2 and num4 > num3 and num4 > num:
    print(num4, "Es el mayor de esos 4 numeros")

#Ejercicio 2

num = int(input("Digite un numero"))
num2 = int(input("Digite un numero"))
num3 = int(input("Digite un numero"))

if num > num2 and num > num3 and num2 > num3:
    print(num, num2, num3)
if num > num2 and num > num3 and num3 > num2:
    print(num, num3, num2)
if num2 > num and num2 > num3 and num > num3:
    print(num2, num, num3)
if num2 > num and num2 > num3 and num3 > num:
    print(num2, num3, num)
if num3 > num2 and num3 > num and num > num2:
    print(num3, num, num2)
if num3 > num2 and num3 > num and num2 > num:
    print(num3, num2, num)

#Ejercicio 3

año = int(input("Digite su año: "))

if año // 4 > 0 and año // 100 > 0:
    print(año, "Es un año bisiesto")
else:
    print(año, "No es un año bisiesto")
